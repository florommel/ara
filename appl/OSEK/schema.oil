{
  "id": "ara-oil-schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON-OIL",
  "description": "JSON version of the OSEK Implementation Language",
  "required": [ "cpu" ],
  "type": "object",
  "properties": {
    "latitude": {
      "type": "number",
      "minimum": -90,
      "maximum": 90
    },
    "longitude": {
      "type": "number",
      "minimum": -180,
      "maximum": 180
    }
  }
}
